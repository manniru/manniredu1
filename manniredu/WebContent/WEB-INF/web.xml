<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>manniredu</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>Sample Servlet</description>
    <display-name>MannireduServlet</display-name>
    <servlet-name>MannireduServlet</servlet-name>
    <servlet-class>com.mannir.edu.MannireduServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MannireduServlet</servlet-name>
    <url-pattern>/manniredu</url-pattern>
  </servlet-mapping>
  
  <servlet><servlet-name>Excel</servlet-name><servlet-class>com.mannir.servlets.Excel</servlet-class></servlet>
  <servlet-mapping><servlet-name>Excel</servlet-name><url-pattern>/excel/*</url-pattern></servlet-mapping>
  
  <servlet><servlet-name>Exams</servlet-name><servlet-class>com.mannir.servlets.Exams</servlet-class></servlet>
  <servlet-mapping><servlet-name>Exams</servlet-name><url-pattern>/exams/*</url-pattern></servlet-mapping>
</web-app>